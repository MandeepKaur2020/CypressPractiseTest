{ 
  "name": "CypressPractise",
  "version": "1.0.0",
  "description": "this is for demo cypress practice",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "merge":"mochawesome-merge cypress/reports/*.json > cypress/reports/output.json",
